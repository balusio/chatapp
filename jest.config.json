{
  "verbose": true,
  "testRegex": "((\\.|/*.)(test))\\.js?$",
  "setupFilesAfterEnv": ["<rootDir>/test/setupTests.js"],
  "moduleNameMapper": {
    "^lib(.*)": "<rootDir>/src/lib$1",
    "^components(.*)": "<rootDir>/src/components",
    "^pages(.*)": "<rootDir>/src/lib",
    "^containers(.*)": "<rootDir>/src/lib"
  } 
}
